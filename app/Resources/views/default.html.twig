                address = $('#letter').val();
                $('#newsletter').val("");
                $.ajax({
                    url: "{{ path('dkk_app_newsletter') }}",
                    type:"POST",
                    data:{email:address},
                    success: function(msg){
                        alert(msg);
                    },
                    error: function(xhr){
                        alert("erreur ajax" + xhr);
                    }
                });
            });
